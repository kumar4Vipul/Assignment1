#factorial of a number..
def factorial(n):
    x=1
    i=0
    while i<n:
        i+=1
        x=x*i
    print(x)
result=factorial(3)    

#output6

#decimal to binary without using inbuilt function.
def decimalTobinary(n):
    if(n>1):
        decimalTobinary(n//2)
    print(n%2)

n=int(input())    

if __name__=='__main__':

    result=decimalTobinary(n)

