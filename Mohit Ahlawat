#binary to decimal conversion.
def binTodeci(x):
    deci,i=0,0
    while(x!=0):

        dec=x%10
        deci=deci+dec*pow(2,i)
        x=x//10
        i+=1

    print(deci)
x=int(input())

result=binTodeci(x)
result

