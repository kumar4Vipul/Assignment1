#program to delete duplicate value in list..
a=[1,2,2,3,3,4,4,5]
b=[]

for i in a:
    if i not in b:
        b.append(i)

        pass    

    else:

        continue

print(b)        

[1, 2, 3, 4, 5]

#prime numbers in given range....

n=int(input())

b=[]

for i in range(n+1):

    if i>1:

        for k in range(2,i):

            if(i%k==0):

                break

        else:

            if (i not in b):

                b.append(i)

print(b)                    

